<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
 	http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
	<bean id="user1" class="assemble.User">
		<constructor-arg index="0" value="爱实打实的气味" />
		<constructor-arg index="1" value="123123" />
		<constructor-arg index="2">
			<list>
				<value>"qqq"</value>
				<value>"qqq"</value>
			</list>
		</constructor-arg>		
	</bean>
	<bean id="user2" class="assemble.User">
		<property name="username" value="爱上打算第二天"></property>
		<property name="password" value="55555"></property>
		<!-- 注入list集合 -->
		<property name="list">
			<list>
				<value>"略略略略"</value>
				<value>"洛克精灵会计"</value>
			</list>
		</property>
	</bean>
</beans>
